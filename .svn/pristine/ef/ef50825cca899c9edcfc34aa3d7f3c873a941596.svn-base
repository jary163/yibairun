    private String password;
    private String appkey;
    private String img;
    private String mobile;
    private String realname;
    private float bonusmoney;//累计收益
    private float investmoney;//正在投资金额
    private float rebatemoney;//累计好友返利
    private int fritotal;//推荐好友数
    private float userinvest;//预期收益
    private float money;//帐户余额
