    private int is_first_cash;//是否第一次取现
    private String code;//邀请码
    private boolean isUploadIdCard;
    

    public String getRealname() {
		return realname;
	}

	public void setRealname(String realname) {
		this.realname = realname;
	}

    public boolean isUploadIdCard() {
        return isUploadIdCard;
    }

    public void setUploadIdCard(boolean isUploadIdCard) {
        this.isUploadIdCard = isUploadIdCard;
    }

    public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getPassword() {
        return password;
    }

    public void setPassword(String password) {
        this.password = password;
    }

    public String getIs_paypwd() {
		return is_paypwd;
	}

